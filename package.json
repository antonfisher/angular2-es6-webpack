{
  "name": "angular2-es6-webpack",
  "description": "Starter Kit for Angular2 with ES6 and Webpack",
  "version": "0.1.0",
  "scripts": {
    "lint": "./node_modules/.bin/standard ./src/**/*.js | ./node_modules/.bin/snazzy",
    "build": "./node_modules/.bin/rimraf dist && ./node_modules/.bin/webpack --progress --profile",
    "start": "npm run build && ./node_modules/.bin/webpack-dev-server --inline --progress --port 8080"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "license": "MIT",
  "author": "Anton Fisher <a.fschr@gmail.com> (http://antonfisher.com)",
  "repository": {
    "type": "git",
    "url": "https://github.com/antonfisher/angular2-es6-webpack.git"
  },
  "bugs": {
    "url": "https://github.com/antonfisher/angular2-es6-webpack/issues"
  },
  "homepage": "https://github.com/antonfisher/angular2-es6-webpack",
  "keywords": [
    "angular2",
    "es6",
    "webpack"
  ],
  "engines": {
    "node": ">5"
  },
  "dependencies": {
    "angular2": "2.0.0-beta.9",
    "autoprefixer": "6.x",
    "babel-core": "6.x",
    "babel-eslint": "6.x",
    "babel-loader": "6.x",
    "babel-plugin-transform-decorators-legacy": "1.x",
    "babel-polyfill": "6.x",
    "babel-preset-es2015": "6.x",
    "babel-preset-stage-0": "6.x",
    "css-loader": "0.x",
    "es6-promise": "3.x",
    "es6-shim": "0.x",
    "extract-text-webpack-plugin": "1.x",
    "html-webpack-plugin": "2.x",
    "node-sass": "3.x",
    "postcss-loader": "0.x",
    "reflect-metadata": "0.x",
    "rimraf": "2.x",
    "rxjs": "5.0.0-beta.3",
    "sass-loader": "3.x",
    "snazzy": "3.x",
    "standard": "6.x",
    "style-loader": "0.x",
    "webpack": "1.x",
    "webpack-dev-server": "1.x",
    "zone.js": "0.x"
  }
}
